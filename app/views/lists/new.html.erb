<h1>新規投稿</h1>

<!-- %= %：HTMLに変換する、動的なコンテンツとして表示（変化）-->
<!-- % %：HTMLに変換しない、静的なコンテンツとして表示（不変） -->
 
<%= form_with model: @list do |f| %>
    <h4>タイトル</h4>
    <%= f.text_field :title %>

    <h4>本文</h4>
    <%= f.text_area :body %>

    <%= f.submit '投稿' %>
<% end %>

<!-- form_with補足 -->
<!-- table（表）タグ直下、tr（行）タグ直下はformが上手く機能しない場合がある。 -->

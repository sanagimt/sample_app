<h1>編集画面</h1>

<%= form_with model: @list do |f| %>
  <!-- form_withの詳細はnew.html.erb参照 -->

    <h4>タイトル</h4>
    <%= f.text_field :title %>

    <h4>本文</h4>
    <%= f.text_area :body %>

    <h4>画像</h4>
    <%= f.file_field :image, accept: "image/*" %>

    <p></p>
    <%= f.submit '保存' %>

<% end %>